<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xxx="using:plc_soldier_avalonia.Models"
        mc:Ignorable="d" d:DesignWidth="1200" d:DesignHeight="540"
        x:Class="plc_soldier_avalonia.MainWindow"
        RequestedThemeVariant="Light"
        Title="PLC Soldier">
  
  <Grid>
    <Grid.RowDefinitions>
      <!-- Menu-template -->
      <RowDefinition Height="20" MinHeight="20"/>
      <!-- Tools-template -->
      <RowDefinition Height="100"/>
      <!-- Splitter-template -->
      <RowDefinition Height="1" MaxHeight="1"/>
      <!-- Main-space-template -->
      <RowDefinition Height="*"/>
    </Grid.RowDefinitions>

    <!-- Menu -->
    <Grid Grid.Row="0">
      <Menu VerticalAlignment="Top" Grid.Row="0">
        <MenuItem Name="File"
                  Header="File">
          <MenuItem Name="NewProject_MenuItem"
                    Header="New Project" />
          <MenuItem Header="Open Project" />
          <Separator />
          <MenuItem Header="Exit" Click="Exit_Click" />
        </MenuItem>
        <MenuItem Header="Edit"></MenuItem>
        <MenuItem Header="View">
          <MenuItem Header="Logical Organizer" Click="LogicalOrganizer_Click" />
          <MenuItem Header="Controller-organizer" Click="ControllerOrganizer_Click" />
          <MenuItem Header="Errors" Click="Errors_Click" />
          <MenuItem Header="Search Results" Click="SearchResults_Click" />
          <MenuItem Header="Watch" Click="Watch_Click" />
          <MenuItem Header="[Central-space]" Click="Central_Click" />
          <MenuItem Header="[Left-bottom-space]" Click="LeftBottom_Click" />
          <MenuItem Header="[Far-Right-space]" Click="FarRight_Click" />
        </MenuItem>
        <MenuItem Header="Search"></MenuItem>
        <MenuItem Header="Logic"></MenuItem>
        <MenuItem Header="Communications"></MenuItem>
        <MenuItem Header="Tools"></MenuItem>
        <MenuItem Header="Window"></MenuItem>
        <MenuItem Header="Help"></MenuItem>
      </Menu>
    </Grid>

    <!-- Tools -->
    <Grid Grid.Row="1">

    </Grid>

    <!-- Splitter -->
    <!-- Thanks to the margins, the capture area increases, 
         and the visible area decreases to the border -->
    <GridSplitter Grid.Row="2"
                  Margin="0, -5"
                  BorderThickness="0, 5"
                  BorderBrush="Transparent"
                  Background="Black"
                  ResizeDirection="Rows"
                  ResizeBehavior="PreviousAndNext"
                  HorizontalAlignment="Stretch"/>

    <!-- Main-space -->
    <Grid Grid.Row="3" Name="LR_Grid">
      <Grid.ColumnDefinitions>
        <!-- Left-space-template -->
        <ColumnDefinition Width="300"/>
        <!-- Splitter-template -->
        <ColumnDefinition Width="1" MaxWidth="1"/>
        <!-- Right-space-template -->
        <ColumnDefinition Width="*"/>
      </Grid.ColumnDefinitions>

      <!-- Left-space -->
      <Grid Grid.Column="0" Name="LULB_Grid">
        <Grid.RowDefinitions>
          <!-- Left-upper-space-template -->
          <RowDefinition Height="*"/>
          <!-- Splitter-template -->
          <RowDefinition Height="1" MaxHeight="1" />
          <!-- Left-bottom-space-template -->
          <RowDefinition Height="220"/>
        </Grid.RowDefinitions>

        <!-- Left-upper-space -->
