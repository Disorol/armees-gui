          <!-- Central-and-right-space-template -->
          <RowDefinition Height="*"/>
          <!-- Splitter-template -->
          <RowDefinition Height="1" MaxHeight="1"/>
          <!-- Bottom-space-template -->
          <RowDefinition Height="150" />
        </Grid.RowDefinitions>

